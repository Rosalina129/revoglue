<html>
	<head>
		<meta charset="utf-8">
		<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
		<link rel="stylesheet" href="./css/material.min.css">
		<script defer src="./js/material.min.js">
		</script>
		<link rel="stylesheet" href="./css/custom.css">
		<link rel="stylesheet" href="./css/image-animation.css">
	</head>
	<body>
		</style>
		<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
			<header class="mdl-layout__header">
				<div class="mdl-layout__header-row">
					<!-- Title -->
					<span class="mdl-layout-title modern-font">
						Creations
					</span>
					<!-- Add spacer, to align navigation to the right -->
					<div class="mdl-layout-spacer">
					</div>
					<!-- Navigation. We hide it in small screens. -->
					<nav class="mdl-navigation mdl-layout--large-screen-only modern-font">
						<a class="mdl-navigation__link" href="./service.html">
							Services
						</a>
						<a class="mdl-navigation__link" href="./creation.html">
							Creations
						</a>
						<a class="mdl-navigation__link" href="./contact.html">
							Contact me
						</a>
						<a class="mdl-navigation__link" href="./site.html">
							Sites
						</a>
					</nav>
				</div>
			</header>
			<main class="mdl-layout__content">
				<div class="page-content mdl-shadow--2dp">
					<!-- Your content goes here -->
					<div class="assets-images">
					<div class="star-image1"><img src="./image/module-apps/star.svg"></img></div>
					</div>
					<div class="contact-main dark-theme-font" align="center" width="100%">
						<div class="contact-container align-center modern-font light-background-font">
							<div class="title-head-text align-center">
								<div class="animation-title">
									My Creations
								</div>
							</div>
							<div class="title-second-text align-center" style="padding-top:1.2em;">
								<div class="animation-title2">
									All my assets are here, and I share some of the interesting things I've
									made!
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="wide-module">
					<!-- Square card -->
					<style>
						.demo-card-square.mdl-card { width: 256px; height: 480px; top:160px; }
						.demo-card-square > .mdl-card__title { color: #fff; background: url('../assets/demos/dog.png')
						bottom right 15% no-repeat #46B6AC; }
					</style>
					<div class="demo-card-square mdl-card mdl-shadow--2dp">
						<div class="mdl-card__title mdl-card--expand">
							<h2 class="mdl-card__title-text modern-font">
								Minecraft Series
							</h2>
						</div>
						<div class="mdl-card__supporting-text">
							Some Minecraft Maps, Addons, Datapacks.
						</div>
						<div class="mdl-card__actions mdl-card--border">
							<a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect modern-font">
								View Catalog
							</a>
						</div>
					</div>
				</div>
			</main>
		</div>
	</body>

</html>