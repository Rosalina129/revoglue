
@keyframes view {
  from {opacity:0;
  top:40px;}
  to {opacity:1;top:0px;}
}
@keyframes view2 {
  0% {opacity:0;
  top:30px;}
  33% {opacity:0;
  top:30px;}
  100% {opacity:1;top:0px;}
}
